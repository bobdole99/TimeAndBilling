@model Project

<h1> Add new project </h1>

<partial name="_ProjectCard" />

<div class="row">
    <div class="col-md-4">
        <form method="post" asp-action="Save" role="form">
            <div class="form-group">
                <div class="col-md-offset-2 col-md-5">
                    <input type="submit" class="btn btn-primary" value="Update" asp-controller="Project" asp-action="Update" asp-route-id="@Model.ProjectID" />
                </div>
            </div>
        </form>
    </div>
</div>