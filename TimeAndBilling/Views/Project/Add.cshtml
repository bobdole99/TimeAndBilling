@model Project

<h1> Add new project </h1>

<div class="row">
    <div class="col-md-4">
        <form method="post" asp-action="Add" role="form">

            <partial name="_ProjectCard" />

            <div class="form-group">
                <div class="col-md-offset-2 col-md-5">
                    <input type="submit" class="btn btn-primary" value="Save" />
                </div>
            </div>
        </form>
    </div>
</div>